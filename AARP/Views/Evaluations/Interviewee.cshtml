@model AARP.Models.Evaluations.IntervieweeProfileViewModel

@{
    Layout = "~/Views/Shared/_EvaluationsLayout.cshtml";
    ViewBag.Title = "Evaluations";
    var message = TempData["Message"] as AARP.Models.MessageViewModel;
    var currentDate = DateTime.Now;
}
<div class="row stats">
    <div class="col-md-6 col-sm-12 col-xs-12">
        <h2>@Html.Partial("_PreviousPage")@Model.IntervieweeName.Trim() @Html.Partial("_SharePage")@Html.Partial("_PrintPdf")</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-3 col-sm-3 col-xs-12">
        <h3>@Model.JobApplicationName</h3>
    </div>
</div>
<br />

@if (message != null)
{
    <div class='alert @(message.Type== AARP.Models.MessageType.Success?"alert-success":"alert-info") alert-dismissible fade in' role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
        <strong>@message.Title !</strong> @message.Content
    </div>
}

<div class="row">
    <div class="col-lg-6 col-md-12">
        <div class="row">
            <div class="col-xs-12">
                @Html.Partial("_IntervieweeResponse", @Model.Interview1)
            </div>  
        </div>
    </div>
    <div class="col-lg-6 col-md-12">
        <div class="row">
            @Html.Partial("_IntervieweeResponse", @Model.Interview2)
        </div>
    </div>
</div>
